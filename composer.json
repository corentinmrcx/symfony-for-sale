{
  "name": "symfony/for_sale",
  "type": "project",
  "description": "Symfony Project",
  "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.15",
    "doctrine/doctrine-migrations-bundle": "^3.4",
    "doctrine/orm": "^3.5",
    "knplabs/knp-paginator-bundle": "^6.9",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.3",
    "stof/doctrine-extensions-bundle": "^1.14",
    "symfony/asset": "7.1.*",
    "symfony/asset-mapper": "7.1.*",
    "symfony/console": "7.1.*",
    "symfony/doctrine-messenger": "7.1.*",
    "symfony/dotenv": "7.1.*",
    "symfony/expression-language": "7.1.*",
    "symfony/flex": "^2",
    "symfony/form": "7.1.*",
    "symfony/framework-bundle": "7.1.*",
    "symfony/http-client": "7.1.*",
    "symfony/intl": "7.1.*",
    "symfony/mailer": "7.1.*",
    "symfony/mime": "7.1.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "7.1.*",
    "symfony/process": "7.1.*",
    "symfony/property-access": "7.1.*",
    "symfony/property-info": "7.1.*",
    "symfony/runtime": "7.1.*",
    "symfony/security-bundle": "7.1.*",
    "symfony/serializer": "7.1.*",
    "symfony/stimulus-bundle": "^2.30",
    "symfony/string": "7.1.*",
    "symfony/translation": "7.1.*",
    "symfony/twig-bundle": "7.1.*",
    "symfony/ux-turbo": "^2.30",
    "symfony/validator": "7.1.*",
    "symfony/web-link": "7.1.*",
    "symfony/yaml": "7.1.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp-shim": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "start": [
          "Composer\\Config::disableProcessTimeout",
          "symfony serve"
        ],
        "test:csfixer": [
          "php vendor/bin/php-cs-fixer fix --dry-run"
        ],
        "fix:csfixer": [
          "php vendor/bin/php-cs-fixer fix"
        ],
        "test:phpstan": [
          "php vendor/bin/phpstan"
        ],
        "test:twig": [
          "php vendor/bin/twig-cs-fixer lint"
        ],
        "fix:twig": [
          "php vendor/bin/twig-cs-fixer fix"
        ],
        "test:yaml": [
          "php bin/console lint:yaml config/"
        ],
        "test": [
          "@test:csfixer", "@test:phpstan", "@test:twig", "@test:yaml"
        ],
        "fix": [
          "@fix:csfixer", "@fix:phpstan", "@fix:twig"
        ],
        "db": [
          "php bin/console doctrine:database:drop --force --if-exists",
          "php bin/console doctrine:database:create",
          "php bin/console doctrine:migrations:migrate --no-interaction",
          "php bin/console doctrine:fixtures:load --no-interaction"
        ]
    },
    "scripts-descriptions": {
      "start": "Lance le serveur de développement Symfony (désactive le timeout de processus puis exécute 'symfony serve').",
      "test:csfixer": "Vérifie le style de code PHP (php-cs-fixer) sans appliquer de modifications (mode --dry-run).",
      "fix:csfixer": "Corrige automatiquement le style de code PHP via php-cs-fixer.",
      "test:phpstan": "Analyse statique du code PHP avec PHPStan.",
      "fix:phpstan": "Génère ou régénère le baseline PHPStan (à utiliser pour recalibrer l’analyse, non pour corriger le code).",
      "test:twig": "Vérifie la conformité des templates Twig avec twig-cs-fixer (mode lint).",
      "fix:twig": "Corrige automatiquement le style des templates Twig.",
      "test:yaml": "Valide la syntaxe des fichiers YAML situés dans le répertoire config/.",
      "test": "Exécute toute la suite d’analyses (style PHP, analyse statique, lint Twig, lint YAML).",
      "fix": "Applique les corrections automatiques disponibles (PHP CS Fixer, baseline PHPStan si choisi, et Twig CS Fixer)."
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "php-cs-fixer/shim": "^3.86",
    "phpro/grumphp-shim": "^2.15",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.3",
    "symfony/browser-kit": "7.1.*",
    "symfony/css-selector": "7.1.*",
    "symfony/debug-bundle": "7.1.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/stopwatch": "7.1.*",
    "symfony/web-profiler-bundle": "7.1.*",
    "vincentlanglet/twig-cs-fixer": "^3.9",
    "zenstruck/foundry": "^2.6"
  }
}
